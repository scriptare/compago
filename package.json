{
  "name": "compago",
  "version": "4.0.0-beta.4",
  "description": "A minimalist MVC framework for modern browsers.",
  "browser": "src/index.js",
  "module": "src/index.js",
  "keywords": [
    "mvc",
    "framework",
    "client-side",
    "frontend"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint src/*.js tests/*.js",
    "coverage:report": "cat ./coverage/lcov.info | codecov",
    "docs:api": "jsdoc2md --configure jsdoc.json > docs/API.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zandaqo/compago.git"
  },
  "homepage": "https://github.com/zandaqo/compago#readme",
  "bugs": {
    "url": "https://github.com/zandaqo/compago/issues"
  },
  "author": "Maga D. Zandaqo <denelxan@gmail.com> (http://maga.name)",
  "license": "MIT",
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**",
      "!**/src/listener.js",
      "!**/node_modules/**",
      "!**/tests/**"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "coverageReporters": [
      "lcov"
    ],
    "roots": [
      "<rootDir>/tests"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/tests/setup.js"
  },
  "dependencies": {
    "fast-deep-equal": "^2.0.1"
  },
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "codecov": "^3.1.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^23.6.0",
    "jsdoc-plugin-false-booleans": "^1.0.0",
    "jsdoc-plugin-named-defaults": "^1.1.1",
    "jsdoc-to-markdown": "^4.0.1"
  }
}
